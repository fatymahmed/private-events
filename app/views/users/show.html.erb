<h1 class="user"><%= @user.name %></h1>
<h2>Events I created</h2>
<%= render @events unless @events.nil? %>
 <div class="row">
  <div class="col-md-6"> 
    <h2>Events I'm attending</h2>
    <h3>Upcoming Events</h3>
    <%= render @upcoming_events unless @upcoming_events.nil? %>
  </div>
  <div class="col-md-6"> 
    <h3>Past Events</h3>
    <%= render @past_events unless @past_events.nil? %>
  </div>
</div>
